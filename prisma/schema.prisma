generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Role {
  id Int @default(autoincrement()) @id
  name String
}

model User {
  id Int @default(autoincrement()) @id
  email String @unique
  name String
  password String
  role_id Int
  active Boolean @default(true)
}
